# Google Analytics Configuration
# Get your GA4 Measurement ID from https://analytics.google.com/
# It should look like: G-XXXXXXXXXX
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Feature Flags
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_SEARCH=true

# Application Settings
REACT_APP_SCHOOL_NAME="Your School Name"
REACT_APP_ACADEMIC_YEAR="2025-2026"
REACT_APP_CONTACT_EMAIL="admin@yourschool.edu"

# Optional: Custom Theme Colors
REACT_APP_PRIMARY_COLOR="#3b82f6"
REACT_APP_SECONDARY_COLOR="#10b981"
